<h1>Add Community Member to Project</h1>
<h3>Project: <%=h @project.name %></h3>

<% form_for([:admin, @project, @community_member_profile], :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <table border="0" cellspacing="0" cellpadding="0" class="form-table">
    <%= render :partial => "form", :locals => { :f => f } %>
   
    <tr>
      <td class="form-submit" colspan="2"><%= f.submit 'Create' %></td>
    </tr>
  </table>
<% end %>

<%= link_to 'Back', admin_project_community_member_profiles_path(@project) %>
