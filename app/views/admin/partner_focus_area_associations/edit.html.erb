<h1>Edit Partner Focus Area
<br/>Partner: <%=h @partner.andand.display_name %></h1>

<% form_for([:admin, @partner, @partner_focus_area_association]) do |f| %>
  <%= f.error_messages %>

<table border="0" cellspacing="0" cellpadding="0" class="form-table">
  <%= render :partial => "form", :locals => { :f => f } %>

  <tr>
    <td class="form-submit" colspan="2"><%= f.submit 'Update' %></td>
  </tr>
</table>
<% end %>

<%= link_to 'Show', [:admin, @partner, @partner_focus_area_association] %> |
<%= link_to 'Back', [:admin, @partner, :partner_focus_area_associations] %>
