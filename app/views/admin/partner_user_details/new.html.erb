<h1>New partner_user_detail</h1>

<% form_for([:admin, @partner_user_detail]) do |f| %>
  <%= f.error_messages %>

<table border="0" cellspacing="0" cellpadding="0" class="form-table">
  <tr>
    <td class="form-label"><%= f.label :necessary %></td>
    <td><%= f.check_box :necessary %></td>
  </tr>
  <tr>
    <td class="form-submit" colspan="2"><%= f.submit 'Create' %></td>
  </tr>
</table>
<% end %>

<%= link_to 'Back', admin_partner_user_details_path %>
