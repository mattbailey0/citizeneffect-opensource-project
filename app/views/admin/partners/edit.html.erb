<h1>Edit Partner</h1>

<% form_for([:admin, @partner], :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

<table border="0" cellspacing="0" cellpadding="0" class="form-table">
  <%= render :partial => "form", :locals => { :f => f } %>

  <tr>
    <td class="form-submit" colspan="2"><%= f.submit 'Update' %></td>
  </tr>
</table>
<% end %>

<%= link_to 'Show', [:admin, @partner] %> |
<%= link_to 'Back', admin_partners_path %>
