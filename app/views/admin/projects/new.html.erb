<h1>New project</h1>

<%= render :partial => "state_progress" %>

<% form_for([:admin, @project], :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <table border="0" cellspacing="0" cellpadding="0" class="form-table">

  <%= render :partial => "form", :locals => { :f => f } %>

  <tr>
    <td class="form-submit" colspan="2">
      <%= f.submit 'Create Project and Continue Editing' %>
    </td>
  </tr>
</table>
<% end %>

<%= link_to 'Back', admin_projects_path %>
