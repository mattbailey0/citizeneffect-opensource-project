<% partner_updates = @project.status_updates_and_approved_project_reports %>

<% if partner_updates.any? %>
<div class="box-rightcol-top"></div>
<div class="box-rightcol-inner status_updates">
  <div class="header">
    <h4>PROJECT UPDATES</h4>
  </div>
  <div id="status_updates">
    <%= render_with_last :partial => "status_update", :collection => partner_updates %>
  </div>
</div>
<div class="box-rightcol-bottom"></div>
<% end %>